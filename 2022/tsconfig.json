{
    "compilerOptions": {
        "sourceMap": false,
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "ESNext",
        "allowJs": false,
        "moduleResolution": "node",
        "alwaysStrict": true,
        "declaration": false,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "allowUnreachableCode": false
    }
}